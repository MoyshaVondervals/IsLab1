{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@reduxjs/toolkit": "^2.9.0",
    "@stomp/stompjs": "^7.2.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.27.4",
    "axios": "^1.12.2",
    "prop-types": "^15.8.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run openapi:gen && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "openapi:pull:json": "curl -sS http://localhost:8080/v3/api-docs -o openapi.json",
    "openapi:pull:yaml": "curl -sS \"http://localhost:8080/v3/api-docs?format=yaml\" -o openapi.yaml",
    "openapi:gen": "rimraf ./src/api && openapi-generator-cli generate -i ./openapi.json -g typescript-axios -o ./src/api --additional-properties=supportsES6=true,withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models,modelPropertyNaming=original",
    "openapi:regen": "npm run openapi:pull:json && npm run openapi:gen"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.9.3"
  }
}
